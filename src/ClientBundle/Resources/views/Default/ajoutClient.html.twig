{% extends "FrontBundle::layout.html.twig" %}
{% block title %}<title>Ajouter un client</title>{% endblock %}
{% block stylesheets %}
{% endblock %}
{% block body %}
    <h1>Ajouter un nouveau client</h1>
    {% for message in app.flashes('success') %}
        <div class="flash-notice">
            {{ message }}
        </div>
    {% endfor %}
    {{ form_start(form) }}
    {{ form_widget(form) }}
    {{ form_end(form) }}
    <a href={{ path('accueil') }}><button>Retour Accueil</button></a>
    <a href={{ path('listeclient') }}><button>Retour Liste Clients</button></a>
{% endblock %}